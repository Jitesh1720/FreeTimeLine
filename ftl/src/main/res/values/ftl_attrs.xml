<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FreeTimeLine">
        <attr name="top_type" format="enum">
            <enum name="sucker" value="0"/>
            <enum name="solid" value="1"/>
            <enum name="hollow" value="2"/>
        </attr>
        <attr name="node_type" format="enum">
            <enum name="solid" value="10"/>
            <enum name="hollow" value="11"/>
        </attr>
        <attr name="bottom_type" format="enum">
            <enum name="solid" value="20"/>
            <enum name="hollow" value="21"/>
        </attr>

        <attr name="line_color" format="reference|color"/>
        <attr name="solid_color" format="reference|color"/>
        <attr name="hollow_color" format="reference|color"/>
        <attr name="sucker_color" format="reference|color"/>
        <attr name="toggle_color" format="reference|color"/>

        <attr name="left_color" format="reference|color"/>
        <attr name="left_size" format="reference|dimension"/>
        <attr name="parent_color" format="reference|color"/>
        <attr name="parent_size" format="reference|dimension"/>
        <attr name="child_color" format="reference|color"/>
        <attr name="child_size" format="reference|dimension"/>

        <attr name="show_toggle" format="boolean"/>
    </declare-styleable>
</resources>